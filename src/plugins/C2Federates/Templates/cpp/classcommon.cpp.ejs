            std::cerr << getSubscribeErrorMessage() << "<%=isinteraction?"Interaction":"Object"%> Class Not Defined" << std::endl;
            return;
        } catch ( ... ) {
            std::cerr << getSubscribeErrorMessage() << "Exception caught ... retry" << std::endl;
        }
    }
    getIsSubscribed() = true;
}

