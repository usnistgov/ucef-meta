<% /*
input properties
----------------
classname       string
lookahead       number
projectName     string
step            number

used in
-------
LabVIEWFederate.js

*/ -%>
{
    "federateName": "<%=classname%>",
    "federationId": "<%=projectName%>",
    "fomFilepath": "conf/<%=classname%>.xml",
    "maxReconnectAttempts": 12,
    "waitReconnectMs": 5000,
    "isLateJoiner": false,
    "stepSize": <%=step%>,
    "lookAhead": <%=lookahead%>,
    "hostAddress": "localhost",
    "outgoingPort": 1234,
    "incomingPort": 9119
}
