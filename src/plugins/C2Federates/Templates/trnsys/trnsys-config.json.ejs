<% /*
input properties
----------------
classname       string
lookahead       number
projectName     string
step            number

used in
-------
TRNSYSFederate.js

*/ -%>
{
    "federateName": "<%=classname%>",
    "federationId": "<%=projectName%>",
    "fomFilepath": "conf/<%=classname%>.xml",
    "maxReconnectAttempts": 12,
    "waitReconnectMs": 5000,
    "isLateJoiner": false,
    "stepSize": <%=step%>,
    "lookAhead": <%=lookahead%>,
    "portNumber": 1345,
    "variableMapping": "conf/Variables.json"
}
