<% /*
input properties
----------------
classname  string
jarfile    string

used in
-------
GridLabFederate.js
LabVIEWFederate.js
TRNSYSFederate.js

*/ -%>
#!/bin/bash
configFile=conf/<%=classname%>.json
if [ $# -eq 1 ] && [ -f $1 ]; then
    configFile=$1
fi

echo using the file $configFile
java -Djava.net.preferIPv4Stack=true -Dlog4j.configurationFile=conf/log4j2.xml -jar <%=jarfile%> $configFile
