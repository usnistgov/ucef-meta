<% /*simobject(name,sharing,delivery,order,attributes,children) */ -%>
<%= indent%>  <objectClass>
<%= indent%>    <name><%= name %></name>
<%= indent%>    <sharing><%= sharing %></sharing>
<%= indent%>    <semantics></semantics><% -%>
<% for(var i=0; i<attributes.length; i++){ %>
<%= indent%>    <attribute>
<%= indent%>      <name><%= attributes[i].name %></name>
<%= indent%>      <dataType><%= attributes[i].parameterType %></dataType>
<%= indent%>      <transportation><%= attributes[i].delivery %></transportation>
<%= indent%>      <order><%= attributes[i].order %></order>
<%= indent%>      <semantics></semantics>
<%= indent%>    </attribute><% -%>
<% }  %>
<% children.forEach(function(child){ %><%- child %><% }); -%>
<%= indent%>  </objectClass>
