<% /*
input properties
----------------
children    array
deliveryXml string
indent	    string of spaces
name        string
orderXml    string
parameters  array
sharingXml  string

used in
-------
DeploymentExporter.js
FederatesExporter.js

*/ -%>
<%= indent%>    <interactionClass>
<%= indent%>        <name><%= name %></name>
<%= indent%>        <sharing><%= sharingXml %></sharing>
<%= indent%>        <transportation><%= deliveryXml %></transportation>
<%= indent%>        <order><%= orderXml %></order><% -%>
<% parameters.forEach(function(param){ -%>
<%   if (!param.inherited) { %>
<%= indent%>        <parameter> 
<%= indent%>            <name><%= param.name %></name>
<%= indent%>            <dataType><%= param.parameterType %></dataType>
<%= indent%>        </parameter><% }}); %>
<% children.forEach(function(child){ %><%- child %><% }); -%>
<%= indent%>    </interactionClass>
